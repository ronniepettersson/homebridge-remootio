        "api_secret_key": {
          "title": "API Secret Key",
          "type": "string",
          "required": true,
          "default": "API Secret Key from App"
        },
        "api_auth_key": {
          "title": "API Authentication Key",
          "type": "string",
          "required": true,
          "default": "API Authentication Key from App"
        }
      }
    }
  }